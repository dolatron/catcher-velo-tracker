{
  "specification": "https://github.com/alloydwhitlock/structure-file-spec",
  "name": "Catcher Velocity Tracker",
  "version": "1.0.0",
  "description": "A workout tracking application for baseball catcher velocity training",
  "encoding": "utf8",
  "repository": {
    "type": "git",
    "url": "https://github.com/username/catcher-velo-tracker"
  },
  "dependencies": {
    "node": ">=18.0.0",
    "react": ">=18.0.0"
  },
  "directories": {
    "src": {
      "description": "Source code directory",
      "contents": {
        "components": {
          "description": "React components",
          "contents": {
            "exercise-row.tsx": "Exercise row component for displaying individual exercises",
            "velo-tracker.tsx": "Main velocity tracking component"
          }
        },
        "data": {
          "description": "Data and configuration files",
          "contents": {
            "programs": {
              "description": "Workout program definitions",
              "contents": {
                "catcher-velo-program.json": "Main velocity program configuration"
              }
            },
            "exercises.ts": "Exercise definitions and configurations",
            "programs.ts": "Program configurations and mappings",
            "types.ts": "TypeScript type definitions",
            "workouts.ts": "Workout type definitions"
          }
        },
        "utils": {
          "description": "Utility functions and helpers",
          "contents": {
            "common.ts": "Common utility functions",
            "program-loader.ts": "Program loading utilities",
            "program-utils.ts": "Program-specific utility functions"
          }
        }
      }
    },
    "public": {
      "description": "Public assets directory",
      "contents": {}
    }
  },
  "metadata": {
    "created": "2024-01-19",
    "creator": "Chris Dolan",
    "license": "MIT",
    "keywords": [
      "workout",
      "baseball",
      "velocity",
      "tracking",
      "react",
      "typescript"
    ]
  }
}
