{
  "specification": "https://github.com/alloydwhitlock/structure-file-spec",
  "name": "Workout Tracker",
  "version": "1.0.0",
  "description": "A workout tracking application for baseball training programs",
  "encoding": "utf8",
  "repository": {
    "type": "git",
    "url": "https://github.com/dolatron/catcher-velo-tracker"
  },
  "dependencies": {
    "node": ">=18.0.0",
    "react": ">=18.0.0"
  },
  "directories": {
    "src": {
      "description": "Source code directory",
      "contents": {
        "app": {
          "description": "Next.js app directory",
          "contents": {
            "fonts": {
              "description": "Local font files",
              "contents": {
                "GeistMonoVF.woff": "Geist Mono variable font",
                "GeistVF.woff": "Geist Sans variable font"
              }
            },
            "favicon.ico": "Site favicon",
            "globals.css": "Global styles",
            "layout.tsx": "Root layout component",
            "page.tsx": "Home page component"
          }
        },
        "common": {
          "description": "Shared utilities and types",
          "contents": {
            "types.ts": "TypeScript type definitions",
            "utils.ts": "Utility functions"
          }
        },
        "components": {
          "description": "React components",
          "contents": {
            "ui": {
              "description": "Shared UI components using shadcn/ui",
              "contents": {
                "alert.tsx": "Alert component",
                "card.tsx": "Card component",
                "checkbox.tsx": "Checkbox component",
                "confirm-modal.tsx": "Confirmation modal component",
                "label.tsx": "Label component"
              }
            },
            "date-picker.tsx": "Date picker component",
            "day-card.tsx": "Day card component",
            "exercise-row.tsx": "Exercise row component",
            "workout-detail-card.tsx": "Workout detail card component",
            "workout-tracker.tsx": "Main workout tracker component"
          }
        },
        "contexts": {
          "description": "React context providers",
          "contents": {
            "program-context.tsx": "Program data provider"
          }
        },
        "lib": {
          "description": "Library code",
          "contents": {
            "utils.ts": "Utility functions"
          }
        },
        "programs": {
          "description": "Program definitions",
          "contents": {
            "driveline-catcher-velo": {
              "description": "Driveline catcher velocity program",
              "contents": {
                "exercises.json": "Exercise definitions",
                "program.json": "Program structure and schedule"
              }
            }
          }
        }
      }
    }
  },
  "metadata": {
    "created": "2024-01-19",
    "creator": "Chris Dolan",
    "license": "MIT",
    "keywords": [
      "workout",
      "baseball",
      "velocity",
      "tracking",
      "react",
      "typescript"
    ]
  }
}
